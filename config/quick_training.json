{
  "data_config": {
    "train_path": "",
    "validation_split": 0.3,
    "k_folds": 1,
    "stratified": true,
    "random_seed": 42
  },
  "training_config": {
    "max_epochs": 50,
    "batch_size": 32,
    "optimization_metric": "pr_auc",
    "verbose": true,
    "log_interval": 5
  },
  "optimizer_config": {
    "type": "differential_evolution",
    "population_size": 20,
    "mutation_factor": 0.8,
    "crossover_prob": 0.9,
    "min_weight": 0.0,
    "max_weight": 2.0
  },
  "early_stopping": {
    "patience": 5,
    "min_delta": 0.001,
    "monitor": "pr_auc",
    "mode": "max"
  },
  "visualization": {
    "enabled": true,
    "output_dir": "./output",
    "formats": ["png"],
    "generate_report": true,
    "dpi": 150
  }
}